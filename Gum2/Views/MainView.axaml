<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Gum2.Views.MainView">
	<Grid ColumnDefinitions="*, 4, *, 4, *">
		<GridSplitter Grid.Column="1" Background="LightGray" ResizeDirection="Columns"/>
		<GridSplitter Grid.Column="3" Background="LightGray" ResizeDirection="Columns"/>
		<TabControl x:Name="LeftTabControl">
		</TabControl>
		<Grid Grid.Column="2" RowDefinitions="*,4,*">
			<TabControl x:Name="MiddleTopTabControl">
			</TabControl>
			<GridSplitter Grid.Row="1" Background="LightGray" ResizeDirection="Rows"/>
			<TabControl Grid.Row="2" x:Name="MiddleBottomTabControl">
			</TabControl>
		</Grid>

		<Grid Grid.Column="4" RowDefinitions="*,4,*">
			<TabControl x:Name="RightTopTabControl">
			</TabControl>
			<GridSplitter Grid.Row="1" Background="LightGray" ResizeDirection="Rows"/>
			<TabControl Grid.Row="2" x:Name="RightBottomTabControl"/>
		</Grid>
		
	</Grid>


  
</UserControl>
